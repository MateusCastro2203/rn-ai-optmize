{
  "name": "rn-ai-optimize",
  "version": "0.1.1",
  "description": "A tool to optimize React Native code using AI",
  "main": "dist/bin/index.js",
  "bin": {
    "rn-ai-optimize": "./dist/bin/index.js"
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "dev": "tsx bin/index.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r src/i18n/*.json dist/src/i18n/",
    "start": "node dist/bin/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-native",
    "cli",
    "ia",
    "performance",
    "openai"
  ],
  "author": "Mateus Castro",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.6.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.0",
    "i18next": "^25.2.1",
    "markdown-it": "^14.1.0",
    "open": "^10.2.0",
    "openai": "^5.10.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.0.14",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
